<!DOCTYPE html>
<html>
<head>
  <title>GithubApiExample</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <nav>
    <ul>
      <li>
        <%= link_to "Search", new_repository_search_path %>
      </li>
      <li>
        <%= link_to "Past Searches", repository_searches_path %>
      </li>
    </ul>
  </nav>

  <main>
    <%= yield %>
  </main>

</body>
</html>
